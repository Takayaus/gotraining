<% @post_announcements.each do |post_announcement| %>
<ul>
    <li>
        <%= link_to post_announcement.title, post_announcement_path(post_announcement) %>
        <p>作成日<%= post_announcement.created_at.strftime("%Y年%-m月%-d日 %-H時%-M分%-S秒") %></p>
        <p>投稿日<%= post_announcement.post.strftime("%Y年%-m月%-d日 %-H時%-M分%-S秒") %></p>
    </li>
</ul>
<% end %>